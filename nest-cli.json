{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "entryFile": "main",
  "assets": [
    {
      "include": "src/public/page/**/*",
      "outDir": "dist/public/page"
    }
  ]
}